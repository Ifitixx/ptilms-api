{
  "name": "ptilms-api",
  "version": "1.0.0",
  "description": "PTiLMS API",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "db:migrate": "dotenv -e .env -- sequelize db:migrate",
    "db:seed": "dotenv -e .env -- sequelize db:seed:all",
    "db:reset": "dotenv -e .env -- sequelize db:migrate:undo:all || true && dotenv -e .env -- sequelize db:migrate && dotenv -e .env -- sequelize db:seed:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "glob": "^11.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.2",
    "nodemailer": "^6.10.0",
    "rimraf": "^6.0.1",
    "sequelize": "^6.37.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "dotenv-cli": "^8.0.0",
    "nodemon": "^3.1.0",
    "sequelize-cli": "^6.6.2"
  }
}